{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": false,
  "palette": {
    "white": "#DDDDDD",
    "black": "#111111",
    "red": "#dc2f02",
    "orange": "#f77f00",
    "yellow": "#FBBA18",
    "grey": "#264653",

    "git-foreground": "p:white",
    "git-background": "#2a9d8f",
    "git-modified": "p:yellow",
    "git-diverged": "p:red",
    "git-ahead": "#B388FF",
    "git-behind": "#B388FB",
    "path-green": "#3EC669",
    "ps-blue":  "#0273B7",
    "ps-lightblue": "#7FD5EA",
    "ubuntu-orange": "#D24413",
    "ubuntu-purple": "#77216F"
    
},
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "powerline",
          "foreground": "p:white",
          "properties": {
            "prefix": "",
            "postfix": "",
            "windows": "\uf871",
            "ubuntu": "<p:ubuntu-orange>ubuntu</>",
            "wsl": "wsl",
            "wsl_separator": ":"
          }
          
        },
        {
          "type": "shell",
          "style": "plain",
          "foreground": "p:white",
          "background": "transparent",
          "properties": {
            "mapped_shell_names": {
              "prefix": "",
              "postfix": "",
              "pwsh": "<p:ps-blue>PS \u276F_</>",
              "bash": "<p:ubuntu-orange>$</>"
            }
          }
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "p:white",
          "properties": {
            "prefix": "",
            "time_format": "3:04 PM"
          }
        },
        {
          "type": "path",
          "style": "diamond",
          "foreground": "p:path-green",
          "properties": {
            "enable_hyperlink": true,
            "prefix": "\uE5FF ",
            "postfix": "",
            "style": "agnoster_full"
          }
        },
        {
          "type": "git",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "p:git-foreground",
          "foreground_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:black{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:white{{ end }}",
            "{{ if gt .Ahead 0 }}p:black{{ end }}",
            "{{ if gt .Behind 0 }}p:white{{ end }}"
          ],
          "background": "p:git-background",
          "background_templates": [
            "{{ if or (.Working.Changed) (.Staging.Changed) }}p:yellow{{ end }}",
            "{{ if and (gt .Ahead 0) (gt .Behind 0) }}p:git-background{{ end }}",
            "{{ if gt .Ahead 0 }}p:git-ahead{{ end }}",
            "{{ if gt .Behind 0 }}p:git-behind{{ end }}"
          ],
          "properties": {
            "fetch_status": true,
            "fetch_stash_count": true,
            "fetch_upstream_icon": true,
            "prefix": "",
            "postfix": "",
            "display_status_detail": true,
            "branch_icon": ""
          }
        },
        {
          "type": "python",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#E4F34A",
          "background": "#29315A",
          "properties": {
            "prefix": "\uE235",
            "postfix": "",
            "display_version": false
          }
        },
        {
          "type": "go",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#7FD5EA",
          "background": "#29315A",
          "properties": {
            "prefix": "\uFCD1",
            "postfix": "",
            "display_version": false
          }
        },
        {
          "type": "node",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#42E66C",
          "background": "#29315A",
          "properties": {
            "prefix": "\uE718",
            "postfix": "",
            "display_version": false
          }
        },
        {
          "type": "ruby",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#E64747",
          "background": "#29315A",
          "properties": {
            "prefix": "\uE791",
            "postfix": "",
            "display_version": false
          }
        },
        {
          "type": "java",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#E64747",
          "background": "#29315A",
          "properties": {
            "prefix": "\uE738",
            "postfix": "",
            "display_version": false
          }
        },
        {
          "type": "julia",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#9B6BDF",
          "background": "#29315A",
          "properties": {
            "prefix": "\uE624",
            "display_version": false
          }
        },
        {
          "type": "battery",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#193549",
          "background": "#ffeb3b",
          "properties": {
            "battery_icon": "",
            "discharging_icon": "\u21E3 ",
            "charging_icon": "\u21E1 ",
            "charged_icon": "\u0095 ",
            "color_background": false,
            "charged_color": "#4caf50",
            "charging_color": "#40c4ff",
            "discharging_color": "#ff5722",
            "prefix": "",
            "postfix": "",
            "display_charging": true
          }
        },
        {
          "type": "executiontime",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "p:white",
          "foreground_templates": [
            "{{ if gt .Ms 4999 }}p:white{{ end }}",
            "{{ if gt .Ms 2000 }}p:black{{ end }}"
          ],
          "background": "p:grey",
          "background_templates": [
            "{{ if gt .Ms 4999 }}p:red{{ end }}",
            "{{ if gt .Ms 2000 }}p:yellow{{ end }}"
          ],
          "properties": {
            "threshold": 150,
            "style": "round",
            
            "prefix": "",
            "postfix": ""
          }
        },
        {
          "type": "exit",
          "style": "plain",
          "properties": {
            "always_enabled": false,
            "template": "{{ if gt .Code 0 }}<p:red>\ue20d {{.Text}}</>{{ else }}<p:path-green>\uf444</>{{ end }}",
            "prefix": " ",
            "postfix": ""
          }
        },
        {
          "type": "shell",
          "style": "plain",
          "background": "transparent",
          "properties": {
            "mapped_shell_names": {
              "prefix": "",
              "postfix": "",
              "pwsh": "<p:ps-lightblue>\u276F</>",
              "bash": "<p:ubuntu-purple>$</>"
            }
          }
        }
      ]
    }
  ]
}