{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "final_space": true,
  "palette": {
    "git-foreground": "#193549",
    "git": "#FFFB38",
    "git-modified": "#FF9248",
    "git-diverged": "#FF4500",
    "git-ahead": "#B388FF",
    "git-behind": "#B388FF",
    "path-green": "#3EC669",
    "ps-blue":  "#0273B7",
    "ps-lightblue": "#7FD5EA",
    "ubuntu-orange": "#D24413",
    "ubuntu-purple": "#34024D",
    "white": "#ffffff",
    "black": "#111111"
},
  "blocks": [
    {
      
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "powerline",
          "foreground": "white",
          "properties": {
            "prefix": "",
            "postfix": "",
            "windows": "\uf871",
            "ubuntu": "ubuntu",
            "wsl": "wsl",
            "wsl_separator": ""
          }
          
        },
        {
          "type": "shell",
          "style": "plain",
          "foreground": "#ffffff",
          "background": "transparent",
          "properties": {
            "mapped_shell_names": {
              "prefix": "",
              "postfix": "",
              "pwsh": "<#0273B7>PS \u276F_</>",
              "bash": "<#D24413>bash</>"
            }
          },
          "postfix": "\u2800"
        },
        {
          "type": "time",
          "style": "plain",
          "foreground": "p:white",
          "properties": {
            "prefix": "",
            "time_format": "3:04 PM"
          }
        },
        {
          "type": "path",
          "style": "diamond",
          "foreground": "p:path-green",
          "properties": {
            "enable_hyperlink": true,
            "prefix": "\uE5FF ",
            "postfix": "",
            "style": "agnoster_full"
          }
        },
        {
          "type": "git",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#43CCEA",
          "background": "#29315A",
          "properties": {
            "prefix": "",
            "postfix": "",
            "display_status_detail": false,
            "branch_icon": ""
          }
        },
        {
          "type": "python",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#E4F34A",
          "background": "#29315A",
          "properties": {
            "prefix": "\uE235",
            "postfix": "",
            "display_version": false
          }
        },
        {
          "type": "go",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#7FD5EA",
          "background": "#29315A",
          "properties": {
            "prefix": "\uFCD1",
            "postfix": "",
            "display_version": false
          }
        },
        {
          "type": "node",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#42E66C",
          "background": "#29315A",
          "properties": {
            "prefix": "\uE718",
            "postfix": "",
            "display_version": false
          }
        },
        {
          "type": "ruby",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#E64747",
          "background": "#29315A",
          "properties": {
            "prefix": "\uE791",
            "postfix": "",
            "display_version": false
          }
        },
        {
          "type": "java",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#E64747",
          "background": "#29315A",
          "properties": {
            "prefix": "\uE738",
            "postfix": "",
            "display_version": false
          }
        },
        {
          "type": "julia",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#9B6BDF",
          "background": "#29315A",
          "properties": {
            "prefix": "\uE624",
            "display_version": false
          }
        },
        {
          "type": "battery",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#9B6BDF",
          "background": "#29315A",
          "properties": {
            "battery_icon": "",
            "discharging_icon": "\u21E3 ",
            "charging_icon": "\u21E1 ",
            "charged_icon": "\u0095 ",
            "color_background": false,
            "charged_color": "#4caf50",
            "charging_color": "#40c4ff",
            "discharging_color": "#ff5722",
            "prefix": "",
            "postfix": "",
            "display_charging": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "executiontime",
          "style": "diamond",
          "trailing_diamond": "\uE0B4",
          "leading_diamond": " \uE0B6",
          "foreground": "#AEA4BF",
          "background": "#29315A",
          "properties": {
            "threshold": 150,
            "style": "round",
            "prefix": "",
            "postfix": ""
          }
        },
        {
          "type": "text",
          "style": "diamond",
          "foreground": "#7FD5EA",
          "properties": {
              "prefix": " ",
              "text": "\u276F",
              "postfix": ""
          }
        }
      ]
    }
  ]
}
